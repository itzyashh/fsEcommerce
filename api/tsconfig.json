{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ES2023",
        "moduleResolution": "Node",
        "outDir": "./dist",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": false,
        "isolatedModules": true,
        "baseUrl": "./",
        "paths": {
            "*": ["node_modules/*"]
        },
        "esModuleInterop": true


    }
}